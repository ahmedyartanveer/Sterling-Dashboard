<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="Python FastCGI"
                 path="*"
                 verb="*"
                 modules="FastCgiModule"
                 scriptProcessor="C:\inetpub\wwwroot\SterlingDashboard\env\Scripts\python.exe|C:\inetpub\wwwroot\SterlingDashboard\env\lib\site-packages\wfastcgi.py"
                 resourceType="Unspecified"
                 requireAccess="Script" />
        </handlers>
    </system.webServer>

    <appSettings>
        <add key="WSGI_HANDLER" value="core.wsgi.application" />
        <add key="PYTHONPATH" value="C:\inetpub\wwwroot\SterlingDashboard" />
        <add key="DJANGO_SETTINGS_MODULE" value="core.settings" />
        <add key="WSGI_LOG" value="C:\inetpub\wwwroot\SterlingDashboard\my_log.txt" />
    </appSettings>
</configuration>